  "keywords": [
    "postcss",
    "css",
    "postcss-plugin"
  ],
  "scripts": {
    "test": "jest && eslint *.js"
  },
  "author": "Nick Odnari <nick@odnari.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/odnari/postcss-at-debug.git"
  },
  "bugs": {
    "url": "https://github.com/odnari/postcss-at-debug/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.4.27"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "postcss": "^8.4.27",
    "jest": "^29.5.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "node": true,
      "es6": true,
      "jest": true
